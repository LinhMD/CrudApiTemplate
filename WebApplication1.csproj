    <ItemGroup>
      <Content Remove="CrudApiTemplate\**" />
    </ItemGroup>
    
    <ItemGroup>
      <ProjectReference Include="CrudApiTemplate\CrudApiTemplate.csproj" />
    </ItemGroup>

</Project>
